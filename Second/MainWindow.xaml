<Window x:Class="Second.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Second"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel>
            <TextBlock Text="Number" HorizontalAlignment="Center"></TextBlock>
            <TextBox x:Name="TextBox1" Height="25" HorizontalAlignment="Center" Width="300"></TextBox>
            <TextBlock Text="Permutation(separator=' ')" HorizontalAlignment="Center"></TextBlock>
            <TextBox x:Name="TextBox2" Height="25" HorizontalAlignment="Center" Width="300"></TextBox>
            <Button x:Name="Button1" Width="300" Height="30" Content="Go" Click="Button1_OnClick"
                    Margin="0 10 0 10"></Button>
            <TextBlock Text="Result" HorizontalAlignment="Center"></TextBlock>
            <TextBox Height="25" IsEnabled="False" HorizontalAlignment="Center" Width="300"></TextBox>
        </StackPanel>
    </Grid>
</Window>
